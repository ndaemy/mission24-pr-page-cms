{
  "kind": "collectionType",
  "collectionName": "participants",
  "info": {
    "singularName": "participant",
    "pluralName": "participants",
    "displayName": "Participant",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "univ": {
      "type": "string",
      "required": true,
      "regex": "^명지대학교|을지대학교|한국외국어대학교 글로벌|한양대학교 에리카$"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^010-\\d{4}-\\d{4}$"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "member",
        "op-member",
        "op-advisor",
        "officer"
      ],
      "default": "member",
      "required": true
    }
  }
}
